  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --mode development --open",
    "start": "node dist/server.js",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "build:client": "webpack --mode development",
    "build:server": "babel --presets env server.js -d dist/",
    "build:dev": "rm -rf dist && concurrently \"yarn build:client\" \"yarn build:server\"",
    "test:dev": "jest -u --coverage",
    "test:watch": "yarn test:dev --watch"
  },
